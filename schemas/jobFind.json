{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "j1"
            ]
        },
        "minSalary": {
            "type": "integer",
            "default": 0,
            "title": "The minSalary Schema",
            "examples": [
                50000
            ],
            "minimum": 0
        },
        "hasEquity": {
            "type": "boolean",
            "title": "The hasEquity Schema",
            "examples": [
                true
            ]
        }
    },
    "additionalProperties": false, 
    "examples": [{
        "title": "j1",
        "minSalary": 50000,
        "hasEquity": true
    }]
}